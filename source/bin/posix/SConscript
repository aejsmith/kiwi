env.KiwiApplication('ln', ['ln.c'])
env.KiwiApplication('ls', ['ls.c'])
env.KiwiApplication('mkdir', ['mkdir.c'])
env.KiwiApplication('mv', ['mv.c'])
env.KiwiApplication('rm', ['rm.c'])
env.KiwiApplication('rmdir', ['rmdir.c'])
env.KiwiApplication('unlink', ['unlink.c'])
